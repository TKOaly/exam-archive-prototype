{
  "name": "exam-archive-new",
  "version": "2.2.0",
  "main": "src/index.ts",
  "author": "cxcorp",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run db:migrate && node dist/index.js",
    "start-dev": "concurrently npm:watch:scss npm:watch:app",
    "build": "tsc && cp -R ./src/views ./dist/views && npm run build:scss && npm run build:autoprefixer",
    "build:scss": "sass --no-source-map -s compressed src/styles/main.scss static/main.css",
    "build:autoprefixer": "postcss -r --use autoprefixer --no-map static/main.css",
    "watch:scss": "sass --watch src/styles/main.scss static/main.css",
    "watch:app": "cross-env NODE_ENV=development nodemon -e ts,js,jsx,json --ignore 'src/views/**/*.jsx' --exec ts-node src/index.ts",
    "db:migrate-dev": "cross-env NODE_ENV=development npm run db:migrate",
    "db:migrate": "knex migrate:latest",
    "seed-from-files-dev": "cross-env NODE_ENV=development node scripts/seed-db-from-files.js",
    "seed-s3-from-db": "cross-env NODE_ENV=development node scripts/seed-s3-from-db.js"
  },
  "dependencies": {
    "aws-sdk": "^2.872.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "content-disposition": "^0.5.3",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "date-fns": "^2.9.0",
    "express": "^4.17.1",
    "express-react-views": "^0.11.0",
    "express-session": "^1.17.0",
    "knex": "^0.21.2",
    "memorystore": "^1.6.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "pg": "^8.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "slugify": "^1.3.6",
    "transliteration": "^2.1.3",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.7.0",
    "@types/content-disposition": "^0.5.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.15.16",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.4.3",
    "@types/multer-s3": "^2.7.7",
    "@types/node": "^10.17.27",
    "@types/uuid": "^8.0.0",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.8",
    "ts-node": "^8.10.2",
    "typescript": "^3.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
